<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical">
    <ListView
        android:id="@+id/ls_show_identify"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_blue_light"
        android:listSelector="@android:color/transparent"
        android:visibility="gone" />

    <com.small.tag.widget.CustomDrawTextView
        android:layout_width="match_parent"
        android:background="@color/colorPrimary"
        android:visibility="gone"
        android:layout_height="wrap_content" />

    <com.small.tag.widget.CircleProgress
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:visibility="gone"
        android:id="@+id/custom_circle_progress"
        app:hint="当前时速"
        app:hintSize="20sp"
        app:unit="Km/h"
        app:valueSize="25sp" />

    <ImageView
        android:id="@+id/iv_object_animation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:src="@drawable/rotation"
        android:visibility="gone" />
        />

    <ImageView
        android:id="@+id/iv_my_ball"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:src="@drawable/rotation"
        android:visibility="gone" />
        />

    <com.small.tag.widget.CircleProgressTwo
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:visibility="gone"
        android:id="@+id/ircle_progress"
        app:hint="当前时速"
        app:hintSize="20sp"
        app:unit="Km/h"
        app:valueSize="25sp" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="pdf查看"
        android:id="@+id/btn_check_pdf"
        android:textSize="@dimen/dp_15"
        />


    <!--<g707175425.pdflib.PdfWebView-->
        <!--android:id="@+id/my_pdf_web_view"-->
        <!--android:layout_width="match_parent"-->
        <!--android:visibility="gone"-->
        <!--android:layout_height="match_parent"/>-->

    <ListView
        android:divider="@null"
        android:id="@+id/listview_main"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="match_parent">
    </ListView>



</LinearLayout>
